alias currPID R0;
alias curr_process_table R1;

currPID = [SYSTEM_STATUS_TABLE + 1];
curr_process_table = PROCESS_TABLE + currPID*16;
[curr_process_table + 9] = 10;      
[curr_process_table + 13] = SP;	              
SP = [curr_process_table + 11]*512;                 

multipush(R0,R1);
R1=3;
R2=currPID;
call MOD_1;
multipop(R0,R1);

call MOD_5;


